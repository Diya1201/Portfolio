{
    "version": 2,
    "builds": [
        {
            "src": "api/sendEmail.js",
            "use": "@vercel/node"
        },
        {
             "src": "index.html",
             "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/sendEmail",
            "dest": "api/sendEmail.js"
        },
        {
             "src": "(/.*)",
             "dest": "index.html"
        }
    ]
}